def linearsearch(l,k):
    for i in range(len(l)):
        if k==l[i]:
            return i
        return -1
l=[]
n=int(input("enter the number of elemetns in the list"))
print("enter the list elements")
for i in range(n):
    l.append(int(input()))
k=int(input("enter the numnber to be searched"))
result=(linearsearch(l,k))
print("elemnt found at postion",result)
if result!=-1:
    print("element found at postion",result)
else:
    print("element not found")
